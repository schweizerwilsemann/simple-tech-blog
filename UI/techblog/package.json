    "@angular/platform-server": "21.0.0",
    "@angular/router": "21.0.0",
    "@angular/ssr": "21.0.0",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "express": "5.1.0",
    "jwt-decode": "4.0.0",
    "marked": "17.0.1",
    "ngx-cookie-service": "21.0.0",
    "ngx-markdown": "21.0.0",
